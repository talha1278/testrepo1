{
  "networkRules": [
    {
      "name": "AllowDNS",
      "priority": 100,
      "ruleCollectionType": "FirewallPolicyFilterRuleCollection",
      "action": { "type": "Allow" },
      "rules": [
        {
          "ruleType": "NetworkRule",
          "name": "AllowDNS",
          "ipProtocols": ["UDP"],
          "sourceAddresses": ["10.1.0.0/16"],
          "destinationAddresses": ["*"],
          "destinationPorts": ["53"]
        }
      ]
    },
    {
      "name": "AllowSMB",
      "priority": 200,
      "ruleCollectionType": "FirewallPolicyFilterRuleCollection",
      "action": { "type": "Allow" },
      "rules": [
        {
          "ruleType": "NetworkRule",
          "name": "AllowSMB",
          "ipProtocols": ["TCP"],
          "sourceAddresses": ["192.168.100.0/24"],
          "destinationAddresses": ["10.2.0.0/16"],
          "destinationPorts": ["445"]
        }
      ]
    }
  ],
  "applicationRules": [
    {
      "name": "AllowTeams",
      "priority": 100,
      "ruleCollectionType": "FirewallPolicyFilterRuleCollection",
      "action": { "type": "Allow" },
      "rules": [
        {
          "ruleType": "ApplicationRule",
          "name": "AllowTeams",
          "protocols": [
            { "protocolType": "Https", "port": 443 }
          ],
          "sourceAddresses": ["192.168.50.0/24"],
          "targetFqdns": ["*.teams.microsoft.com"],
          "fqdnTags": [],
          "httpHeadersToInsert": [],
          "terminateTLS": false
        }
      ]
    },
    {
      "name": "AllowGitHub",
      "priority": 200,
      "ruleCollectionType": "FirewallPolicyFilterRuleCollection",
      "action": { "type": "Allow" },
      "rules": [
        {
          "ruleType": "ApplicationRule",
          "name": "AllowGitHub",
          "protocols": [
            { "protocolType": "Https", "port": 443 }
          ],
          "sourceAddresses": ["10.3.0.0/16"],
          "targetFqdns": ["*.github.com"],
          "fqdnTags": [],
          "httpHeadersToInsert": [],
          "terminateTLS": false
        }
      ]
    }
  ]
}
